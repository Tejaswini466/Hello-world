#include<stdio.h>
void main()
{
    char s1[10],s2[10];
    int i=0,k;
    printf("Enter string 1: ");
    gets(s1);
    printf("Enter string 2: ");
    gets(s2);
    while(s1[i]==s2[i])
    {
        if(s1[i]=='\0')
          break;
        i++;
    }
    k=s1[i]-s2[i];
    if(k==0)
      printf("Both strings are same");
    else
      printf("Both strings are different");
}
