//7/30: Palindrome
#include<stdio.h>  
int main()    
{    
int n,rem,sum=0,temp;    
printf("Enter the number: ");    
scanf("%d",&n);    
temp=n;    
while(n>0)    
{    
rem=n%10;    
sum=(sum*10)+rem;    
n=n/10;    
}    
if(temp==sum)    
printf("Palindrome number ");    
else    
printf("Not palindrome");   
return 0;  
}  
