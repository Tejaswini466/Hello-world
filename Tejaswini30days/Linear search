//4/30
#include <stdio.h>
int main()
{
    int n,a[20],found=0,i,key;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    printf("Enter array elements: ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the key to be searched\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            printf("Element found at index %d\n",i);
            found=1;
            break;
        }
    }
    if(found==0)
    {
        printf("Element  not found");
    }
}
